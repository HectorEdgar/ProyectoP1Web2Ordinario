@model IEnumerable<ProyectoP1Web2.Models.Hora>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Agregar nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HoraInicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HoraFin)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.HoraInicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HoraFin)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.IdHora }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.IdHora }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.IdHora })
        </td>
    </tr>
}

</table>
